@inproceedings{Muniz2013a,
	abstract = {RESTful Web services have become a popular standard that sees the web hosts as servers that offer resources using the HTTP protocol. The SERIN - Semantic RESTFul Interfaces applies Object Orientation Interfaces paradigm to define conceptual models that describe RESTful services syntactically and semantically. A host that implements an interface offer the specified RESTful services to access resources that are intances of the classes specified in the interface. The SERIN proposal adds annotations in the classes of OWL ontologies to determine which operations (add, modify, delete or retrieve) the RESTful web services must perform. In this paper, the SERIN proposal was simplified and also is presented a full sample program that implements the proposed Semantic RESTFul Interfaces.},
	author = {Muniz, Bruno de Azevedo and Chaves, Laura Maria and Lira, Hermano Albuquerque and Dantas, Jos{\'{e}} Renato Villela and Farias, Pedro Porf{\'{i}}rio Muniz},
	booktitle = {Proceedings of the IADIS International Conference on WWW/Internet},
	file = {:home/renato/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Muniz et al. - 2013 - SERIN - AN APROACH TO SPECIFY SEMANTIC ABSTRACT INTERFACES IN THE CONTEXT OF RESTFUL WEB SERVICES.pdf:pdf},
	keywords = {ontology,restful web services,semantic interfaces,semantic web,semantic web services},
	pages = {187--194},
	publisher = {IADIS Press},
	title = {{SERIN - An Approach to Specify Semantic Abstract Interfaces in the Context of RESTful Web Services}},
	year = {2013}
}
