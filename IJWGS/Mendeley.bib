@inproceedings{Sengupta2011,
	abstract = {We present a new approach to adding closed world reasoning to the Web Ontology Language OWL. It transcends previous work on circumscriptive description logics which had the drawback of yielding an undecidable logic unless severe restrictions were imposed. In particular, it was not possible, in general, to apply local closure to roles. In this paper, we provide a new approach, called grounded circumscrip- tion, which is applicable to SROIQ and other description logics around OWL without these restrictions. We show that the resulting language is decidable, and we derive an upper complexity bound. We also provide a decision procedure in the form of a tableaux algorithm.},
	address = {Galway, Ireland},
	author = {Sengupta, Kunal and Krisnadhi, Adila and Hitzler, Pascal},
	booktitle = {The Fifth International Conference on Web Reasoning and Rule Systems (RR)},
	doi = {10.1007/978-3-642-23580-1_22},
	editor = {Rudolph, Sebastian and Gutierrez, Claudio},
	file = {:home/renato/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sengupta et al. - 2011 - Local Closed World Semantics Grounded Circumscription for OWL.pdf:pdf},
	pages = {263--268},
	publisher = {Springer Berlin Heidelberg},
	title = {{Local Closed World Semantics : Grounded Circumscription for OWL}},
	volume = {6902},
	year = {2011}
}
